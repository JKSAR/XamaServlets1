<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
	
 	<servlet>
		<servlet-name>ServletInput</servlet-name>
		<display-name>ServletInput</display-name>
		<description></description>
		<servlet-class>json.xama1.controller.ServletInput</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
 	<servlet>
		<servlet-name>XamaServlet</servlet-name>
		<display-name>XamaServlet</display-name>
		<description></description>
		<servlet-class>json.xama1.controller.XamaServlet</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>	
	
	<servlet-mapping>
		<servlet-name>ServletInput</servlet-name>
		<url-pattern>/servletInput</url-pattern>
	</servlet-mapping>	
	
	<servlet-mapping>
		<servlet-name>XamaServlet</servlet-name>
		<url-pattern>/xama</url-pattern>
	</servlet-mapping>		
	
</web-app>
